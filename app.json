{
  "expo": {
    "name": "Centro-SIS",
    "slug": "centro-sis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-blanco.jpg",
    "scheme": "centrosisfrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
        "adaptiveIcon": {
          "backgroundColor": "#2D3C59",
          "foregroundImage": "./assets/images/logo.png"
        },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2D3C59",
          "dark": {
            "backgroundColor": "#2D3C59"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
