{
  "expo": {
    "name": "Centro SIS",
    "slug": "centro-sis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-centro.png",
    "scheme": "centrosis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "android": {
      "package": "com.estonianport.centrosis",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-centro.png",
        "backgroundColor": "#ffffff"
      }
    },

    "ios": {
      "bundleIdentifier": "com.estonianport.centrosis",
      "buildNumber": "1",
      "supportsTablet": true
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-centro.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-centro.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],

    "assetBundlePatterns": [
      "**/*"
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
